<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://services.infraest.softprog.pucp.edu.pe/" targetNamespace="http://services.infraest.softprog.pucp.edu.pe/" version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="obtenerPorIdDirector" type="tns:obtenerPorIdDirector" />
