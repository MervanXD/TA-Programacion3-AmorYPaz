    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerPorIdDirectorResponse">
    <xs:sequence>
      <xs:element minOccurs="0" name="return" type="tns:ugel" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ugel">
    <xs:sequence>
      <xs:element name="activo" type="xs:boolean" />
      <xs:element name="cantidadIE" type="xs:int" />
